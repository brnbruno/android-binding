<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidbinding">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidBinding">
        <activity android:name=".ActivityFragmentKt"></activity>
        <activity
            android:name=".MainActivityKt"
            android:label="@string/title_activity_main_kt"
            android:theme="@style/Theme.AndroidBinding.NoActionBar" />
        <activity
            android:name=".MainActivityKT"
            android:label="@string/title_activity_main_k_t"
            android:theme="@style/Theme.AndroidBinding.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>